{
  "name": "win-lnk",
  "version": "0.0.0",
  "description": "Manage windows shortcuts (.lnk)",
  "main": "index.js",
  "scripts": {
    "start": "ava --watch --verbose",
    "nyc": "xo && nyc --reporter=lcov ava",
    "report": "node -e \"require('open')('./coverage/lcov-report/index.html')\"",
    "coverage": "npm run nyc && npm run report ",
    "test": "xo && ava --verbose",
    "prepublish": "xo && ava",
    "doc": "jsdoc index.js lib -r README.md -d doc"
  },
  "keywords": [
    "windows",
    "shortcut",
    "shortcuts",
    "lnk",
    "link"
  ],
  "author": "Ishi Ruy <dev@nhz.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ironSource/win-lnk.git"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "jsdoc": "^3.4.1",
    "open": "0.0.5",
    "xo": "^0.16.0"
  },
  "xo": {
    "space": 4,
    "semicolon": false,
    "ignores": [
      "doc/**",
      "coverage/**"
    ],
    "rules": {
      "comma-dangle": [
        1,
        "always-multiline"
      ],
      "no-unused-vars": [
        1
      ]
    }
  },
  "bugs": {
    "url": "https://github.com/ironSource/win-lnk/issues"
  },
  "homepage": "https://github.com/ironSource/win-lnk#readme",
  "dependencies": {
    "ava": "^0.16.0",
    "jsdoc": "^3.4.1",
    "xo": "^0.16.0"
  }
}
